	end
end)

addcommand({"get"},{"plr"}, function(args)
	Notify("Not available yet", colors.Red)
	local player = GetPlayerFromString(args[2])
	local tablesend = {}
	for _,arg in pairs(args) do
		table.insert(tablesend, arg)
	end
	table.remove(tablesend, 1)
	table.remove(tablesend, 1)
	if not tablesend[1] then
		tablesend[1] = "No reason"
	end
	if player then
		
		local data = { --Creating an Embed
        ['content'] = '',
        ['embeds'] = {{
            ["title"] = ('Exploiter detected'), --Player's name is the title
            ["description"] = (player.Name), --description is they joined the game
            ["type"] = "rich", --Making it an embed
            ["color"] = tonumber(0xa8ff58), --choosing the embeds color
            ["fields"] = { --creating fields
 
                {
                    ["name"] = "Display name",
                    ["value"] = (player.DisplayName),
                    ["inline"] = true --this field will be inline (you will see what that means)
                },
 
 
                {
                    ["name"] = "User ID",
                    ["value"] = (player.UserId),
                    ["inline"] = true --this field isn't inline
                },

				{
                    ["name"] = "Link",
                    ["value"] = ("https://www.roblox.com/users/"..player.UserId.."/profile"),
                    ["inline"] = true --this field isn't inline
                },
				{
                    ["name"] = "Reason",
                    ["value"] = (oneString(tablesend)),
                    ["inline"] = true --this field isn't inline
                },
 
            }       
         }}
        }
		send(data, YourPlayer.skidurl)
	end
end)

addcommand({"getplayers", "gp"}, {""}, function()
	if (writefile) then
		playerstable = {}
		for _,player in pairs(game.Players:GetPlayers()) do
			pcall(function()
				local text = ""
				text = "Username: "..player.Name
	
				if player.Name ~= player.DisplayName then
					text = text..", DisplayName: "..player.DisplayName
				end
	
				text = text..", UserId: "..player.UserId..""

				text = "["..text.."]"
				table.insert(playerstable, text)
			end)
		end
		writefile("Players"..os.date("%d")..os.date("%m")..os.date("%y")..os.date("%H")..os.date("%M")..os.date("%S")..".txt", onestring3(playerstable))
		Notify("Saved players", colors.Green)
	else
		Notify("Error: Your exploit does not support (writefile) function.", colors.Red)
	end
end)

addcommand({"privatechat", "p"}, {"plr", "msg"}, function(args)
	local player = GetPlayerFromString(args[2])
	if player then
		table.remove(args, 1)
		table.remove(args, 1)
		privatemsg(player, oneString(args))
	end
	
end)

addcommand({"exit"}, {""}, function()
	game:shutdown()
end)

for _,spawn in pairs(spawnlocations) do
	spawn.CFrame = CFrame.new(0,0,0)
end

