local Remotes = ReplicatedStorage:FindFirstChild('Remotes') or ReplicatedStorage
local ShootEvent = Remotes.ShootEvent
local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()

local idHitSound = "1455817260"

local Bindable = Instance.new('BindableEvent')
function createSoundEffect(id, volume)
    coroutine.wrap(function()
        local sound = Instance.new("Sound")
        sound.SoundId = id
        sound.Volume = volume or 1
        SoundService:PlayLocalSound(sound)
        sound.Ended:Wait(0.5)
        sound:Destroy()
    end)()
end

GUI.DAMAGE_INDICATORS = {
	HitmarkerFrame = Instance.new("Frame"),
	Hitmarker = Instance.new("ImageLabel"),
    AlignHit = Instance.new("UIListLayout"),
	DmgContainer = Instance.new("Frame"),
	Dmg1 = Instance.new("TextLabel")
}

