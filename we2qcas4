function bring2(Player, pos)
	if Player == nil or not Player.Character then return end
	local oldteam = game.Players.LocalPlayer.TeamColor.Color
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	local tchanged = false
	if game.Players.LocalPlayer.Team.Name == "Guards" then
		tchanged = true
		workspace.Remote.TeamEvent:FireServer("Medium stone grey")
	end
	workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single.Hammer.ITEMPICKUP)
	if tchanged == true then
		workspace.Remote.TeamEvent:FireServer("Bright blue")
	end
	local CHAR = game.Players.LocalPlayer.Character
	CHAR.Humanoid.Name = "1"
	local c = CHAR["1"]:Clone()
	c.Name = "Humanoid"
	c.Parent = CHAR
	CHAR["1"]:Destroy()
	game.Workspace.CurrentCamera.CameraSubject = CHAR
	CHAR.Humanoid.DisplayDistanceType = "None"
	CHAR.Animate.Disabled = true
	wait()
	CHAR.Animate.Disabled = false
	
	local tool = game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") or game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("M9")
	tool.Parent = CHAR
	local STOP = 0
	repeat wait()
		STOP = STOP + 1
		local get = Player.Character:FindFirstChild("HumanoidRootPart") or Player.Character:FindFirstChild("Torso")
		get.CFrame = pos * CFrame.new(0, 0, -0.75)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	
	until (not game.Players.LocalPlayer.Character:FindFirstChild("M9") and not game.Players.LocalPlayer.Character:FindFirstChild("Hammer") or not game.Players.LocalPlayer.Character.HumanoidRootPart or STOP > 500) and STOP > 3
	tool:Destroy()
	for i = 1,5 do
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	end
	
	workspace.Remote.loadchar:InvokeServer(nil, oldteam)
end


local function criminpos(Player, pos)
	if Player == nil or not Player.Character then return end
    local savedcolor = game.Players.LocalPlayer.TeamColor.Color
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single.Hammer.ITEMPICKUP)
	if not game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") then
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver.M9.ITEMPICKUP)
	end
	local CHAR = game.Players.LocalPlayer.Character
	CHAR.Humanoid.Name = "1"
	local c = CHAR["1"]:Clone()
	c.Name = "Humanoid"
	c.Parent = CHAR
	CHAR["1"]:Destroy()
	game.Workspace.CurrentCamera.CameraSubject = CHAR
	CHAR.Animate.Disabled = true
	wait()
	CHAR.Animate.Disabled = false
	CHAR.Humanoid.DisplayDistanceType = "None"
	local tool = game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") or game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("M9")
	tool.Parent = CHAR
	local STOP = 0
	repeat wait()
		STOP = STOP + 1
		local get = Player.Character:FindFirstChild("HumanoidRootPart") or Player.Character:FindFirstChild("Torso")
		get.CFrame = pos * CFrame.new(0, 0, -0.75)
		spawnlocations[6].CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		spawnlocations[6].Transparency = 0
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	until (Player.TeamColor.Name == "Really red" or not game.Players.LocalPlayer.Character:FindFirstChild("M9") and not game.Players.LocalPlayer.Character:FindFirstChild("Hammer") or not game.Players.LocalPlayer.Character.HumanoidRootPart or STOP > 500) and STOP > 3
	for i = 1,5 do
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
		local get = Player.Character:FindFirstChild("HumanoidRootPart") or Player.Character:FindFirstChild("Torso")
		spawnlocations[6].CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		spawnlocations[6].Transparency = 0
	end
	local lastCam = workspace.CurrentCamera.CFrame
	workspace.Remote.loadchar:InvokeServer(nil, savedcolor)
	workspace.CurrentCamera.CFrame = lastCam
	wait()
	workspace.CurrentCamera.CFrame = lastCam
	spawnlocations[6].CFrame = CFrame.new(0,0,0)
end


function bringinpos(Player, pos)
	if Player == nil or not Player.Character then return end
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single.Hammer.ITEMPICKUP)
	if not game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") then
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver.M9.ITEMPICKUP)
	end
	local CHAR = game.Players.LocalPlayer.Character
	CHAR.Humanoid.Name = "1"
	local c = CHAR["1"]:Clone()
	c.Name = "Humanoid"
	c.Parent = CHAR
	CHAR["1"]:Destroy()
	game.Workspace.CurrentCamera.CameraSubject = CHAR
	CHAR.Animate.Disabled = true
	wait()
	CHAR.Animate.Disabled = false
	CHAR.Humanoid.DisplayDistanceType = "None"
	local tool = game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") or game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("M9")
	tool.Parent = CHAR
	local STOP = 0
	repeat wait()
		STOP = STOP + 1
		local get = Player.Character:FindFirstChild("HumanoidRootPart") or Player.Character:FindFirstChild("Torso")
		get.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -0.45)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	until (Player.TeamColor.Name == "Really red" or not game.Players.LocalPlayer.Character:FindFirstChild("M9") and not game.Players.LocalPlayer.Character:FindFirstChild("Hammer") or not game.Players.LocalPlayer.Character.HumanoidRootPart or not Player.Character:FindFirstChild("Torso") or not Player.Character:FindFirstChild("Torso").Parent or STOP > 500 or Player.TeamColor.Name == "Really red") and STOP > 3
	for i = 1,5 do
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
	end
	workspace.Remote.loadchar:InvokeServer()
end

states.SPAMPOS = {1018, 99, 2390}
addLoop(0.2, function()
	if states.SPAM_ARRESTING ~= nil then
		if states.SPAM_ARRESTING.TeamColor.Name == "Bright orange" then
			bring2(states.SPAM_ARRESTING, CFrame.new(unpack(states.SPAMPOS)))
		else
			if states.SPAM_ARRESTING.TeamColor.Name ~= "Really red" then
				bring2(states.SPAM_ARRESTING, CFrame.new(unpack(states.SPAMPOS)))
			end
		end
	end
end)

addLoop(0, function()
	if states.SPAM_ARRESTING ~= nil then
		if states.SPAM_ARRESTING.TeamColor.Name == "Bright orange" or states.SPAM_ARRESTING.TeamColor.Name == "Really red" then
			spawnlocations[7].CFrame = CFrame.new(0,0,0)
			spawnlocations[7].Size = Vector3.new(0.5,0.5,0.5)
			spawnlocations[7].Transparency = 1
			spawnlocations[7].CanCollide = true
		else
			local get = states.SPAM_ARRESTING.Character:FindFirstChild("Head") or states.SPAM_ARRESTING.Character:FindFirstChild("Torso")
			if (Vector3.new(unpack(states.SPAMPOS)) - get.Position).Magnitude < 20 then
				spawnlocations[7].CFrame = get.CFrame
				spawnlocations[7].Size = Vector3.new(0.5,0.5,0.5)
				spawnlocations[7].Transparency = 1
				spawnlocations[7].CanCollide = true
			end
			
		end
		
	end
end)




function removeIndex(array, index)
	for i,v in pairs(array) do
		if index == v then
			table.remove(array, v)
		end
	end
end

-- ADMIN SCRIPTING
function get_command(fromchat, cmd5, adminplayer, player)
	if adminplayer == false then
		coroutine.wrap(function()
			local obligatoryprefix = false
		if fromchat == true then
			obligatoryprefix = true
		end
		local args = {}
		local pattern = "%S+"
	
		for word in string.gmatch(string.lower(cmd5), pattern) do
			table.insert(args, word)
		end

		if args[1] == "/e" then
			table.remove(args, 1)
		end
	
		if args[1] == "/w" then
			table.remove(args, 1)
			table.remove(args, 1)
		end

		if obligatoryprefix == true then
			if string.sub(args[1], 0,1) ~= "." then
				return false
			end
		end
	
		if string.sub(args[1], 0,1) == "." then
			args[1] = string.sub(args[1],2,#args[1])
		end
	
		local funct = getcmdfunction(args[1])
		if funct ~= nil then
			pcall(funct, args)
		else
			Notify("Invalid command", colors.Red)
		end
		
	
		end)()
	elseif adminplayer == true then
		coroutine.wrap(function()
			local obligatoryprefix = false
		if fromchat == true then
			obligatoryprefix = true
		end
		local args = {}
		local pattern = "%S+"
	
		local args = {}
		local pattern = "%S+"
	
		for word in string.gmatch(string.lower(cmd5), pattern) do
			table.insert(args, word)
		end

		if args[1] == "/e" then
			table.remove(args, 1)
		end
	
		if args[1] == "/w" then
			table.remove(args, 1)
			table.remove(args, 1)
		end

		if obligatoryprefix == true then
			if string.sub(args[1], 0,1) ~= "." then
				return false
			end
		end
	
		if string.sub(args[1], 0,1) == "." then
			args[1] = string.sub(args[1],2,#args[1])
		end

		local funct = getadminscmdfunction(args[1])
		if funct ~= nil then
			pcall(funct, unpack({args, player}))
		end
		
		end)()
	end
end

game.Players.LocalPlayer.Chatted:Connect(function(msg)
	get_command(true, msg, false)
end)

GUI.BAR.cmdbar11.FocusLost:connect(function(enter)
	local close = UDim2.new(-0.500, 0, 0, 910)
	if enter then
		get_command(false, GUI.BAR.cmdbar11.Text, false)
        GUI.BAR.cmdbar11.Text = " "
	end
	sc.tweenservice:Create(GUI.BAR.Bar, TweenInfo.new(0.3), {Position = close}):Play()
	GUI.BAR.cmdbar11.Text = " "
end)

game:GetService("UserInputService").InputBegan:connect(function(input, process)
	if process then return end

	local open = UDim2.new(0, 0, 0, 910)
	
	if input.KeyCode == Enum.KeyCode.Period then
		sc.tweenservice:Create(GUI.BAR.Bar, TweenInfo.new(0.3), {Position = open}):Play()
		GUI.BAR.cmdbar11:CaptureFocus()
		wait(0.1)
		GUI.BAR.cmdbar11.Text = " "
	end

	if input.KeyCode == Enum.KeyCode.K then
		if states.COMMANDS_GUI == true then
			sc.tweenservice:Create(GUI.CMDGUI.CommandsGUI, TweenInfo.new(0.3), {Position = UDim2.new(-0.500, 0, 0, 673)}):Play()
			states.COMMANDS_GUI = false
		else
			sc.tweenservice:Create(GUI.CMDGUI.CommandsGUI, TweenInfo.new(0.3), {Position = UDim2.new(0, 0, 0, 673)}):Play()
			states.COMMANDS_GUI = true
		end
	end

	if input.KeyCode == Enum.KeyCode.P then
		GUI.OUTPUT.Output.Visible = not GUI.OUTPUT.Output.Visible
	end
end)

--if true will check your messages too
local spyOnMyself = true
--if true will chat the logs publicly (fun, risky)
local public = false
--if true will use /me to stand out
local publicItalics = true
--customize private logs
local privateProperties = {
	Color = Color3.fromRGB(0,228,255); 
	Font = Enum.Font.SourceSansBold;
	TextSize = 18;
}
--////////////////////////////////////////////////////////////////
local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local saymsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
local getmsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")
local instance = (_G.chatSpyInstance or 0) + 1
_G.chatSpyInstance = instance

local function onChatted(p,msg)
	if _G.chatSpyInstance == instance then
		if p==player and msg:lower():sub(1,4)=="/spy" then
			states.SPY_CHAT = not states.SPY_CHAT
			wait(0.3)
			StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
		elseif states.SPY_CHAT and (spyOnMyself==true or p~=player) then
			msg = msg:gsub("[\n\r]",''):gsub("\t",' '):gsub("[ ]+",' ')
			local hidden = true
			local conn = getmsg.OnClientEvent:Connect(function(packet,channel)
				if packet.SpeakerUserId==p.UserId and packet.Message==msg:sub(#msg-#packet.Message+1) and (channel=="All" or (channel=="Team" and public==false and Players[packet.FromSpeaker].Team==player.Team)) then
					hidden = false
				end
			end)
			wait(1)
			conn:Disconnect()
			if hidden and states.SPY_CHAT then
				if public then
					saymsg:FireServer((publicItalics and "/me " or '').."[".. p.Name .."]: "..msg,"All")
				else
					privateProperties.Text = "{SPY}: [".. p.Name .."]: "..msg
					StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
				end
			end
		end
	end
end

for _,p in ipairs(Players:GetPlayers()) do
	p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end
Players.PlayerAdded:Connect(function(p)
	p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end)

function unloopkill(arg)
	if not arg or arg == "all" then
		Notify("Cleared loopkills",  colors.Green)
		loopkills = {}
		states.LOOPINGALL = false
		states.ANTI_TIME_OUT = false
	else
		if GetPlayerFromString(arg) then
			removeplayer(GetPlayerFromString(arg), loopkills)
			Notify("Unlooping "..GetPlayerFromString(arg).Name, colors.Red)
		else
			Notify("Player not found", colors.Red) return
		end
	end
end

function loopkill(arg)
	if not arg then Notify("Player not found", colors.Red) return end
	if arg == "all" then
		Notify("Looping all", colors.Green) 
		states.ANTI_TIME_OUT = true
	else
		if GetPlayerFromString(arg) then
			addplayer(GetPlayerFromString(arg), loopkills)
			Notify("Looping "..GetPlayerFromString(arg).Name, colors.Green)
		else
			Notify("Player not found", colors.Red) return
		end
	end
end

local chatFrame = player.PlayerGui.Chat.Frame
chatFrame.ChatChannelParentFrame.Visible = true
chatFrame.ChatBarParentFrame.Position = chatFrame.ChatChannelParentFrame.Position+UDim2.new(UDim.new(),chatFrame.ChatChannelParentFrame.Size.Y)

table.insert(connections.system, game:GetService("RunService").Stepped:Connect(function()
	if states.ANTI_SPAM_ARREST_LAG == true or states.AFK == true then
		pcall(function()
			for i,player in pairs(game.Players:GetPlayers()) do
				if player ~= game.Players.LocalPlayer then
					for i, part in pairs(player.Character:FindFirstChild("Head"):GetChildren()) do
						if part.Name == "handcuffedGui" then
							part:Destroy()
						end
					end
				else
					if player == game.Players.LocalPlayer then
						local guis = {}
						for i, part in pairs(player.Character:FindFirstChild("Head"):GetChildren()) do
							if part.Name == "handcuffedGui" then
								table.insert(guis, part)
							end
						end
						if #guis > 5 then
							for _,part in pairs(guis) do
								part:Destroy()
							end
						end
					end
					
				end
			end
		end)
	end
end))

table.insert(connections.system, game:GetService("RunService").Stepped:Connect(function()
	if states.ANTI_ARMOR_SPAM == true or states.AFK == true then
		pcall(function()
			for i,player in pairs(game.Players:GetPlayers()) do
				for i, part in pairs(player.Character:GetChildren()) do
					if part.Name == "vest" then
						part:Destroy()
					end
				end
			end
		end)
	end
end))
local CMDVar = {
	Waiting = {},
	Warned = {},
	Warned2 = {}
}
function privatemsg(plr, msg)
	local args = {
		[1] = "/w "..plr.Name.." "..msg,
		[2] = "All"
	}

	game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
end

function onestring2(tab)
	local text = ""
	for i = 1,#tab do
		tab[i] = "."..tab[i]
		if i == 1 then
			text = text..tab[i]
		elseif i > 1 then
			text = text..", "..tab[i]
		end
	end
	return text
end

function onestring3(tab)
	local text = ""
	for i = 1,#tab do
		if i == 1 then
			text = text..tab[i]
		elseif i > 1 then
			text = text..", "..tab[i]
		end
	end
	return text
end
function tellCmds(player)
	print("starting")
	if not CMDVar.Waiting[player.UserId] then
		addplayer(player, CMDVar.Waiting)
		local printing = 1
		local msgs = {}
		local messages = {[1] = {}}
		local using = 1
		for i = 1,#cmds2list do
			if #messages[using] > 5 then
				using = using + 1
				table.insert(messages, {})
			end

			table.insert(messages[using], cmds2list[i])
		end
		for _,msg in pairs(messages) do
			privatemsg(player, onestring2(msg))
			wait(0.1)
		end
		coroutine.wrap(function()
			wait(80)
			removeplayer(player, CMDVar.Waiting)
			removeplayer(player, CMDVar.Warned)
		end)()
	else
		if not CMDVar.Warned[player.UserId] then
			addplayer(player, CMDVar.Warned)
			privatemsg(player, "Please stop abusing .cmds command.")
		end
	end
end
-- ADMIN PLAYERS
addadmincommand({"cmds"}, {""}, function(args, Player)
	tellCmds(Player)
end)

function gunsPlayer(Player, toPos)
	if Player == nil then return end
	local savedcf = GetRO()
	
	local oldplayerpos = toPos or GetROFrom(Player.Character:FindFirstChild("Torso"))
	wait(0.1)
	workspace.Remote.loadchar:InvokeServer()
	game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = savedcf
	workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single.Hammer.ITEMPICKUP)
	if not game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") then
		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver.M9.ITEMPICKUP)
	end
	local CHAR = game.Players.LocalPlayer.Character
	CHAR.Humanoid.Name = "1"
	local c = CHAR["1"]:Clone()
	c.Name = "Humanoid"
	c.Parent = CHAR
	CHAR["1"]:Destroy()
	game.Workspace.CurrentCamera.CameraSubject = CHAR
	CHAR.Animate.Disabled = true
	wait()
	CHAR.Animate.Disabled = false
	CHAR.Humanoid.DisplayDistanceType = "None"
	local tool = game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer") or game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("M9")
	tool.Parent = CHAR
	local STOP = 0
	local PLAYER_CHAR = Player.Character
	local PLAYER_BACKPACK = Player.Backpack
	repeat wait(.1)
		STOP = STOP + 1
		local get = Player.Character:FindFirstChild("HumanoidRootPart") or Player.Character:FindFirstChild("Torso")
		get.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -0.75)
	until (not game.Players.LocalPlayer.Character:FindFirstChild("M9") and not game.Players.LocalPlayer.Character:FindFirstChild("Hammer") or not game.Players.LocalPlayer.Character.HumanoidRootPart or not get or not game.Players.LocalPlayer.Character.HumanoidRootPart.Parent or not get.Parent or STOP > 500) and STOP > 3
	for i = 1,40 do
		if PLAYER_CHAR.Parent ~= game.Workspace or PLAYER_BACKPACK:FindFirstChild("Remington 870") then
		    break
		end
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(825, 100, 2254)
	end
	for i = 1,40 do
		if PLAYER_CHAR.Parent ~= game.Workspace or PLAYER_BACKPACK:FindFirstChild("M4A1") then
		    break
		end
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(852, 100, 2254)
	end
	for i = 1,10 do
		if PLAYER_CHAR.Parent ~= game.Workspace then
		    break
		end
		wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldplayerpos
	end
	workspace.Remote.loadchar:InvokeServer()
	game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = savedcf
end

function gunsPlayer2(Player, toPos)
	if Player == nil then return end
	local giving = {
		"M4A1",
		"Remington 870",
		"AK-47",
		"M9"
	}
	local oldplayerpos = toPos or GetROFrom(Player.Character:FindFirstChild("Torso"))

	for i = 1,#giving do
		local savedcf = GetRO()
		workspace.Remote.loadchar:InvokeServer()
		game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = savedcf

		workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver[giving[i]].ITEMPICKUP)
		local tool1 = game.Players.LocalPlayer.Backpack:FindFirstChild(giving[i]) or game.Players.LocalPlayer.Character:FindFirstChild(giving[i])
		for _,child in pairs(tool1:GetChildren()) do
			if child:IsA("Script") or child:IsA("LocalScript") then
				child.Disabled = true
			end
		end

		local CHAR = game.Players.LocalPlayer.Character
		CHAR.Humanoid.Name = "1"
		local c = CHAR["1"]:Clone()
		c.Name = "Humanoid"
		c.Parent = CHAR
		CHAR["1"]:Destroy()
		game.Workspace.CurrentCamera.CameraSubject = CHAR
		CHAR.Animate.Disabled = true
		wait()
		CHAR.Animate.Disabled = false
		CHAR.Humanoid.DisplayDistanceType = "None"
		tool1.Parent = CHAR
		local STOP = 0
		local PLAYER_CHAR = Player.Character
		local PLAYER_BACKPACK = Player.Backpack

		repeat wait()
			STOP = STOP + 1
			local get = Player.Character:FindFirstChild("HumanoidRootPart") or Player.Character:FindFirstChild("Torso")
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = get.CFrame
		until (not game.Players.LocalPlayer.Character:FindFirstChild(giving[i]) or not game.Players.LocalPlayer.Character.HumanoidRootPart or not get or not game.Players.LocalPlayer.Character.HumanoidRootPart.Parent or not get.Parent or STOP > 500) and STOP > 3
		workspace.Remote.loadchar:InvokeServer()
	    game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = savedcf
	end
	bring(Player, oldplayerpos)
end

addadmincommand({"re", "refresh"}, {""}, function(args, Player)
	local isAutoguns = false
	if autogunsadmin[Player.UserId] then
		removeplayer(Player, autogunsadmin)
		isAutoguns = true
	end
	local char = Player.Character
	local oldpos = (char:FindFirstChild("Torso") or char:FindFirstChild("Head")).CFrame
	kill2(Player, true)
	Player.CharacterAdded:Wait()
	Player.Character:WaitForChild("Torso")
	gunsPlayer(Player, oldpos)
	if isAutoguns == true then
		addplayer(Player, autogunsadmin)
	end
end)

addadmincommand({"guns"}, {"plr/me"}, function(args, Player)
	if not args[2] or args[2] == "me" then
		gunsPlayer(Player, nil)
	else
		if GetPlayerFromString(args[2]) then
			gunsPlayer(GetPlayerFromString(args[2]), nil)
		end
	end
	
end)

addadmincommand({"autoguns","aguns"}, {"on/off"}, function(args, Player)
	if args[2] == "on" then
		addplayer(Player, autogunsadmin)
	else
		if args[2] == "off" then
			removeplayer(Player, autogunsadmin)
		else
			if not args[2] then
				if autogunsadmin[Player.UserId] then
					removeplayer(Player, autogunsadmin)
				else
					addplayer(Player, autogunsadmin)
				end
			end
		end
	end
	
end)

addadmincommand({"goto", "to"}, {"plr"}, function(args, Player)
	if GetPlayerFromString(args[2]) then
		bring(Player, (GetPlayerFromString(args[2]).Character:FindFirstChild("Torso") or GetPlayerFromString(args[2]).Character:FindFirstChild("Head")).CFrame)
	end
end)

addadmincommand({"kill", "k"}, {"plr"}, function(args,Player)
	kill2(GetPlayerFromString(args[2]), true)
end)

addadmincommand({"loopkill", "lk"}, {"plr"}, function(args, Player)
	loopkill(args[2])
end)

addadmincommand({"unloopkill","unlk"}, {"plr"}, function(args, Player)
	unloopkill(args[2])
end)

addadmincommand({"spamarrest", "sa"}, {"plr"}, function(args, Player)
	if args[2] then
		if args[2] == "off" then
			states.SPAM_ARRESTING = nil
		else
			local plr = GetPlayerFromString(args[2])
			if plr then
				states.SPAM_ARRESTING = plr
				local displayname = plr.DisplayName.." "
				if plr.Name == plr.DisplayName then
					displayname = ""
				end
	
				local displaymsg = displayname.."@"..plr.Name
			end
			
		end
	end
end)

addadmincommand({"bring"}, {"plr"}, function(args, Player)
	bring(GetPlayerFromString(args[2]), (Player.Character:FindFirstChild("Torso") or Player.Character:FindFirstChild("Head")).CFrame)
end)

