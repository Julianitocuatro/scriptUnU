addcommand({"admin"}, {"plr"}, function(args)
	local plr = GetPlayerFromString(args[2])
	if plr ~= nil and not admins[plr.UserId] then
		admins[plr.UserId] = {plr = plr}
	end
	Notify(string.upper(plr.Name).." is ADMIN", colors.Green)
	privatemsg(plr, "You got admin. Use .cmds to view commands.")
	wait()
	privatemsg(plr, "Remember to use USERNAME, not DISPLAY NAME.")
end)

addcommand({"unadmin"}, {"plr"}, function(args)
	local plr = GetPlayerFromString(args[2])
	if plr ~= nil and admins[plr.UserId] then
		admins[plr.UserId] = nil
	end
	Notify(string.upper(plr.Name).." is not ADMIN", colors.Red)
end)

