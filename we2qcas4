function send(msg, webhook)
	if (syn.request) then
		syn.request({
			Url = webhook,
			Method = "POST",
			Headers = {
				["Content-Type"] = "application/json"
			},
			Body = game.HttpService:JSONEncode(msg)
		})
	end
end
