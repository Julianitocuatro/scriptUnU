GUI.DAMAGE_INDICATORS.Dmg1.TextWrapped = true
function hitmarker()
	coroutine.wrap(function()
		local doing = true

		local Clone = GUI.DAMAGE_INDICATORS.HitmarkerFrame:Clone()
		local NewHit = Clone:WaitForChild("Hitmarker")
		
		Clone.Position = UDim2.new(0,Mouse.X,0,Mouse.Y)
		Clone.Parent = ScreenGui
		NewHit.Size = UDim2.new(0, 22, 0, 22) 
		Clone.Visible = true
		
		coroutine.wrap(function()
			repeat wait()
				pcall(function()
					Clone.Position = UDim2.new(0,Mouse.X,0,Mouse.Y)
				end)
			until doing == false
		end)()
		
		sc.tweenservice:Create(NewHit, TweenInfo.new(0.1), {ImageTransparency = 0}):Play()
		NewHit:TweenSize(
			UDim2.new(0, 28, 0, 28),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quad,
			0.05,
			true
		)
		wait(0.07)
		sc.tweenservice:Create(NewHit, TweenInfo.new(0.1), {ImageTransparency = 1}):Play()
		wait(0.3)
		doing = false
		game:GetService("Debris"):AddItem(clone, 0.3)
	end)()
end

function randomizeUdim2(udim2, range)
	return udim2 + UDim2.new(math.random(1,range), 0, math.random(1,range), 0)
end

function damageIndicator(hit, gun, player)
	coroutine.wrap(function()
		if not hit then return end
		local damage = 0

		if gun.Name == "AK-47" then
			damage = 11
		else
			if gun.Name == "Remington 870" then
				damage = 15
			else
				if gun.Name == "M9" then
					damage = 10
				else
					if gun.Name == "M4A1" then
						damage = 11
					else
						if gun == "sniper" then
							damage = 100
						end
					end
				end
			end
		end
		local head = false

		if hit.Name == "Head" then
			head = true
		end


		if hit.Name ~= "HumanoidRootPart" and hit.Name ~= "Head" and hit.Name ~= "Left Arm" and hit.Name ~= "Left Leg" and hit.Name ~= "Right Arm" and hit.Name ~= "Right Leg" and hit.Name ~= "Torso" then
			hit = player.Character:FindFirstChild("Torso")
		end
		local gui = Instance.new("BillboardGui", hit)
		local uilist = Instance.new("UIListLayout", gui)
		uilist.HorizontalAlignment = Enum.HorizontalAlignment.Center
		uilist.SortOrder = Enum.SortOrder.LayoutOrder
		uilist.VerticalAlignment = Enum.VerticalAlignment.Center
		gui.Size = UDim2.new(0,300,0,300)
		gui.AlwaysOnTop = true
		gui.ClipsDescendants = false
		local init = UDim2.new(0.498, 0,0.499, 0)
		local finalsize = UDim2.new(0, 47, 0, 23)
		local newdmg = GUI.DAMAGE_INDICATORS.DmgContainer:Clone()
		newdmg.Parent = gui
		
		local Positions = {
			UDim2.new(0.767, 0, 0.389, 0),
			UDim2.new(0.65, 0, 0.755, 0),
			UDim2.new(0.201, 0, 0.356, 0),
			UDim2.new(0.272, 0, 0.753, 0),
			UDim2.new(0.743, 0, 0.173, 0),
			UDim2.new(0.165, 0, 0.496, 0),
			UDim2.new(0.502, 0, 0.205, 0)
		}
	
		local posi = Positions[math.random(1, #Positions)]
		
		local clone = newdmg:WaitForChild("Dmg1")
		clone.Text = "-"..damage
		if head == true then
			clone.TextColor3 = Color3.fromRGB(255,0,0)
		end
		clone.Position = init
		clone.Visible = true
		
		coroutine.wrap(function()
			wait(0.15)
			clone.TextTransparency = 0
			clone.TextStrokeTransparency = 0.5
		end)()
		
		clone:TweenPosition(
			posi,
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Quad,
			0.45,
			true
		)
		wait(0.55)
		clone:TweenSize(
			UDim2.new(0, 0, 0, 0),
			Enum.EasingDirection.InOut,
			Enum.EasingStyle.Linear,
			0.2,
			true
		)
		game:GetService("Debris"):AddItem(clone, 0.3)
	end)()
	
end

Bindable.Event:Connect(function(bullets, gun)
	if states.DAMAGE_INDICATORS == true then
		local ShotHit = false

		for _,bullet in pairs(bullets) do
			local Hit = bullet.Hit
	
			local Limb = Hit.Parent:FindFirstChildOfClass('Humanoid') ~= nil
			local Accessory = Hit.Parent.Parent:FindFirstChildOfClass('Humanoid') ~= nil
	
			if Limb then
				local Player = Players:GetPlayerFromCharacter(Hit.Parent)
				if not loopkills[Player.UserId] then
					if Player.TeamColor ~= LocalPlayer.TeamColor then
						ShotHit = true
						createSoundEffect("rbxassetid://1347140027", 1)
						hitmarker()
						damageIndicator(Hit, gun, Player)
						break
					end
				end
	
				
			elseif Accessory then
				local Player = Players:GetPlayerFromCharacter(Hit.Parent.Parent)
				if not loopkills[Player.UserId] then
					if Player.TeamColor ~= LocalPlayer.TeamColor then
						ShotHit = true
						createSoundEffect("rbxassetid://1347140027", 1)
						hitmarker()
						damageIndicator(Hit, gun, Player)
						break
					end
				end
			end
		end
	end
	
end)

local OldNameCall
OldNameCall = hookmetamethod(game, '__namecall', function(self, ...)
	local method = getnamecallmethod()
	local args = {...}

	if method == 'FireServer' and self == ShootEvent then
		Bindable.Fire(Bindable, unpack(args))
	end

	return OldNameCall(self, ...)
end)
addLoop(0, function()
	if game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") and states.CURSOR == true then
		if game:GetService("UserInputService").MouseIconEnabled == true then
			game:GetService("UserInputService").MouseIconEnabled = false
			Cursor.Visible = true
		end
	else
		if game:GetService("UserInputService").MouseIconEnabled == false then
			game:GetService("UserInputService").MouseIconEnabled = true
			Cursor.Visible = false
		end
	end
end)


addLoop(0, function()
	if states.TIMESTOP == true then
		coroutine.wrap(function()
			pcall(function()
				workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
				local Gun = game.Players.LocalPlayer.Backpack["Remington 870"] or game.Players.LocalPlayer.Character["Remington 870"]
				
				local function LagSpike()
					if not Gun then return end
					game.ReplicatedStorage.ShootEvent:FireServer(states.CRASHTABLE, Gun)
				end
				
				LagSpike()
			end)
		end)()
	end
	
end)

